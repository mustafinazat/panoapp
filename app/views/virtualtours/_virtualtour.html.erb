  <div class="col-sm-6 col-md-6 col-lg-4 col-xl-4">
    <div class="thumbnail">


  <%= link_to virtualtour, class: 'lightbox',data: { turbolinks: false }  do %>

 <img src=" <%= virtualtour.panoramas.first.image.url(:thumb) %>" >

         <div class="caption">
            <h3><%= virtualtour.title %> от</h3>
            <h4><%= virtualtour.user.nickname %></h4>
            </div>
             <%= raw virtualtour.tags.map(&:name).map{ |t| link_to t, tag_path(t), class: 'badge badge-primary' }.join(' ') %>
 <% end %>

	 </div>
 </div>
