  <div class="card">
    <div class="card-header" role="tab" id="heading<%= faq.id %>">
      <h5 class="mb-0">
        <a data-toggle="collapse" href="#collapse<%= faq.id %>" aria-expanded="true" aria-controls="collapse<%= faq.id %>">
          <%= faq.question %>
        </a>

               <%= link_to "x", faq, remote: true, data: { method: "delete" } , class: "faq-link btn btn-danger" %>
       
      </h5>
    </div>
    <div id="collapse<%= faq.id %>" class="collapse" role="tabpanel" aria-labelledby="heading<%= faq.id %>">
      <div class="card-body">
<%= faq.answer.html_safe %>

      </div>
    </div>
  </div>


